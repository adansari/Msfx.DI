<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>Adil Ansari</Authors>
    <PackageProjectUrl>https://github.com/adansari/Msfx.DI</PackageProjectUrl>
    <RepositoryType>Public</RepositoryType>
    <PackageTags>Dependency Injection, Core, .Net, DotNet</PackageTags>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Description>A Conventional .net and .net core dependency injection framework that will empower developers to write loosely coupled components.</Description>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <Version>1.0.1</Version>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Attributes\InjectForTypeAttribute.cs" />
  </ItemGroup>

</Project>
